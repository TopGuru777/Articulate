    <summary>A wonderful Blog engine built on Umbraco</summary>
    <tags>blog umbraco oss tags markdown richtext</tags>
    <copyright>$copyright$</copyright>
    <iconUrl>https://raw.githubusercontent.com/Shazwazza/Articulate/master/assets/Icon_64.png</iconUrl>
    <dependencies>
      <dependency id="UmbracoCms.Web" version="[8.0.2, 9.0.0)" />
      <dependency id="xmlrpcnet" version="[3.0.0.266, 4.0.0)" />
      <dependency id="xmlrpcnet-server" version="[3.0.0.266, 4.0.0)" />
      <dependency id="Argotic.Core" version="2008.0.2" />
      <dependency id="MarkdownDeep.NET" version="1.5" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Articulate.dll" target="lib\net45\" />
    <file src="App_Plugins\Articulate\**\*" target="content\App_Plugins\Articulate" />
    <file src="media\articulate\original\*.jpg" target="content\media\articulate\original" />
    <file src="media\articulate\original\*.png" target="content\media\articulate\original" />
